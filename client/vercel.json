{
    "version": 2,
    "builds": [
      {
        "src": "client/src/App.jsx",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "client/build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*.(js|css|map|ico|png|svg|json))",
        "dest": "/client/build/$1"
      },
      {
        "src": "/.*",
        "dest": "/client/build/index.html"
      }
    ]
  }
  